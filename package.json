{
  "name": "ngx-timeago",
  "description": "Live updating timestamps in Angular 5+.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihym/ngx-timeago.git"
  },
  "bugs": {
    "url": "https://github.com/ihym/ngx-timeago/issues"
  },
  "author": "Vasilis Diakomanolis <b.diakomanolis@gmail.com>",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular5",
    "timeago",
    "ngx-timeago"
  ],
  "peerDependencies": {
    "@angular/core": ">=5.2.11",
    "rxjs": ">=5.5.11"
  },
  "devDependencies": {
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": ">=5.2.11",
    "rxjs": ">=5.5.11",
    "typescript": "~2.6.2"
  },
  "scripts": {
    "build": "tsc && ngc"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "keepLifecycleScripts": true,
    "lib": {
      "entryFile": "src/index.ts"
    },
    "dest": "./dist"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "/dist"
  ]
}
